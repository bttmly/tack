{
  "name": "tack",
  "version": "0.1.0",
  "description": "Build HTML strings",
  "main": "dist/tack.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "coffee -o ./dist src/tack.coffee && coffee -o ./test test/spec.coffee",
    "watch": "coffee -w -o ./dist src/tack.coffee",
    "test": "mocha --compilers coffee:coffee-script/register -R spec test/spec.coffee"
  },
